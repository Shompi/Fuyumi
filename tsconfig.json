{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ESNext",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "allowUnreachableCode": false,
    "watch": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./InteractionCommands/*",
    "./Listeners/*/*",
    "./MessageCommands/*/*"
  ]
}