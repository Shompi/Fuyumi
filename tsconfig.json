{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ESNext",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "allowUnreachableCode": false,
    "watch": false,
		"moduleResolution": "Node",
		"outDir": "./js"
  },
  "exclude": [
    "node_modules",
		"Classes",
		"Configs",
		"GameImages"
  ],
  "include": [
    "./InteractionCommands",
		"./Listeners",
		"./MessageCommands"
  ],
}